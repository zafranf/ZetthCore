{
    "name": "zafranf/zetthcore",
    "description": "ZetthCMS Core Admin Panel",
    "type": "library",
    "require": {
        "php": "^7.2",
        "ext-imagick": "^3.4",
        "doctrine/dbal": "^2.10",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.4",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^5.2",
        "laravel/ui": "^2.5",
        "rap2hpoutre/laravel-log-viewer": "^1.6",
        "santigarcor/laratrust": "^6.0",
        "spatie/laravel-translation-loader": "^2.6",
        "yajra/laravel-datatables-oracle": "~9.0",
        "zafranf/helpers": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zafran Fadilah",
            "email": "surel@zafran.id"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ZetthCore\\": "src/app"
        },
        "classmap": [
            "src/app/",
            "src/database/seeds",
            "src/database/factories"
        ],
        "files": [
            "src/app/helpers.php",
            "src/app/queries.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZetthCore\\Providers\\AppServiceProvider"
            ],
            "aliases": {
                "DataTables": "Yajra\\DataTables\\Facades\\DataTables",
                "Image": "Intervention\\Image\\Facades\\Image",
                "Laratrust": "Laratrust\\LaratrustFacade",
                "Browser": "Jenssegers\\Agent\\Facades\\Agent"
            }
        }
    },
    "scripts": {
        "post-update-cms": [
            "@php artisan zetth:link"
        ]
    }
}
